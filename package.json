{
  "name": "books",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:web": "webpack-dev-server --config webpack.config.js --mode=development --disable-host-check",
    "start": "yarn start:web --progress --colors --open",
    "prebuild": "rm -rf build",
    "build": "webpack --progress --colors --mode=production",
    "build:types": "tsc --emitDeclarationOnly",
    "types": "tsc --noEmit",
    "test": "jest",
    "test:pre-commit": "CI=true yarn test --findRelatedTests",
    "lint": "tslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "yarn lint",
      "yarn test:pre-commit"
    ]
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "testRegex": "(^src.*/__tests__/.*|(\\.|/)(test))\\.(ts|tsx|js|jsx)$",
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.18.0",
    "csstype": "^2.6.2",
    "fast-memoize": "^2.5.1",
    "formik": "^1.4.2",
    "history": "^4.7.2",
    "moment": "^2.23.0",
    "normalizr": "^3.3.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.1",
    "react": "^16.8.3",
    "react-css-loaders": "^0.0.5",
    "react-dom": "^16.8.3",
    "react-google-login": "^5.0.2",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-sizes": "^1.0.4",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "styled-components": "^4.1.3",
    "styled-system": "^4.0.0",
    "typesafe-actions": "^3.1.0",
    "yup": "^0.26.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^24.0.9",
    "@types/node": "^10.12.18",
    "@types/ramda": "^0.25.47",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-modal": "^3.8.1",
    "@types/react-redux": "^7.0.0",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-tether": "^0.5.5",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.3.2",
    "@types/styled-components": "^4.1.10",
    "@types/styled-system": "^3.2.2",
    "@types/webpack": "^4.4.25",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.0",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^0.23.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
